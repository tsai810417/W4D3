<h1>Input Username & Password</h1>
  <form action="<%= sessions_url %>" method="post">

    <label for="username">Username</label>
      <input
        type="text"
        name="user[username]">

    <br>
    <label for="password">Password</label>
      <input
        type="password"
        name="user[password]">

    <input type="submit" value="Log In">
  </form>
